            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AwActivity"
            android:label="@string/title_activity_aw" >
        </activity>
        <activity
            android:name=".IgaActivity"
            android:label="@string/title_activity_iga" >
        </activity>
        <activity
            android:name=".BakmiActivity"
            android:label="@string/title_activity_bakmi" >
        </activity>
    </application>

</manifest>
